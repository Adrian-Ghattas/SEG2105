<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LogoSelectorActivity">

    <TextView
        android:id="@+id/selectLogoText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Select a new Logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.102" />

    <ImageView
        android:id="@+id/teamid01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="184dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="25dp"
        android:onClick="setTeamIcon"
        app:layout_constraintBottom_toTopOf="@+id/teamid04"
        app:layout_constraintEnd_toStartOf="@+id/teamid02"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_logo_01" />

    <ImageView
        android:id="@+id/teamid02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="91dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="26dp"
        android:onClick="setTeamIcon"
        app:layout_constraintBottom_toTopOf="@+id/teamid05"
        app:layout_constraintEnd_toStartOf="@+id/teamid03"
        app:layout_constraintStart_toEndOf="@+id/teamid01"
        app:layout_constraintTop_toBottomOf="@+id/selectLogoText"
        app:srcCompat="@drawable/ic_logo_02" />

    <ImageView
        android:id="@+id/teamid03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginTop="183dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="26dp"
        android:onClick="setTeamIcon"
        app:layout_constraintBottom_toTopOf="@+id/teamid00"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/teamid02"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_logo_03" />

    <ImageView
        android:id="@+id/teamid04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="272dp"
        android:onClick="setTeamIcon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/teamid05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teamid01"
        app:srcCompat="@drawable/ic_logo_04" />

    <ImageView
        android:id="@+id/teamid05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="272dp"
        android:onClick="setTeamIcon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/teamid00"
        app:layout_constraintStart_toEndOf="@+id/teamid04"
        app:layout_constraintTop_toBottomOf="@+id/teamid02"
        app:srcCompat="@drawable/ic_logo_05" />

    <ImageView
        android:id="@+id/teamid00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="272dp"
        android:onClick="setTeamIcon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/teamid05"
        app:layout_constraintTop_toBottomOf="@+id/teamid03"
        app:srcCompat="@drawable/ic_logo_00" />
</androidx.constraintlayout.widget.ConstraintLayout>